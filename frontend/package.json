{
  "name": "timemanager-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Time Manager SaaS Frontend - React/TypeScript/Vite",
  "author": "Trinity Team <trinity@epitech.eu>",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
