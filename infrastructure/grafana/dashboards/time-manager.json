{
  "id": null,
  "uid": "timemanager-main",
  "title": "Time Manager",
  "tags": ["timemanager"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "type": "row",
      "title": "Services",
      "collapsed": false
    },
    {
      "id": 2,
      "gridPos": {"h": 3, "w": 4, "x": 0, "y": 1},
      "type": "stat",
      "title": "Backend",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "up{job=\"backend\"}", "legendFormat": "Status"}],
      "fieldConfig": {
        "defaults": {
          "thresholds": {"mode": "absolute", "steps": [{"value": 0, "color": "red"}, {"value": 1, "color": "green"}]},
          "mappings": [{"type": "value", "options": {"0": {"text": "DOWN"}, "1": {"text": "UP"}}}]
        }
      },
      "options": {"graphMode": "none", "colorMode": "background"}
    },
    {
      "id": 3,
      "gridPos": {"h": 3, "w": 4, "x": 4, "y": 1},
      "type": "stat",
      "title": "Traefik",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "up{job=\"traefik\"}", "legendFormat": "Status"}],
      "fieldConfig": {
        "defaults": {
          "thresholds": {"mode": "absolute", "steps": [{"value": 0, "color": "red"}, {"value": 1, "color": "green"}]},
          "mappings": [{"type": "value", "options": {"0": {"text": "DOWN"}, "1": {"text": "UP"}}}]
        }
      },
      "options": {"graphMode": "none", "colorMode": "background"}
    },
    {
      "id": 4,
      "gridPos": {"h": 3, "w": 4, "x": 8, "y": 1},
      "type": "stat",
      "title": "cAdvisor",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "up{job=\"cadvisor\"}", "legendFormat": "Status"}],
      "fieldConfig": {
        "defaults": {
          "thresholds": {"mode": "absolute", "steps": [{"value": 0, "color": "red"}, {"value": 1, "color": "green"}]},
          "mappings": [{"type": "value", "options": {"0": {"text": "DOWN"}, "1": {"text": "UP"}}}]
        }
      },
      "options": {"graphMode": "none", "colorMode": "background"}
    },
    {
      "id": 5,
      "gridPos": {"h": 3, "w": 4, "x": 12, "y": 1},
      "type": "stat",
      "title": "Requetes/min",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "sum(rate(http_requests_total[1m])) * 60"}],
      "fieldConfig": {"defaults": {"unit": "short"}},
      "options": {"graphMode": "area", "colorMode": "value"}
    },
    {
      "id": 6,
      "gridPos": {"h": 3, "w": 4, "x": 16, "y": 1},
      "type": "stat",
      "title": "Erreurs %",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "(sum(rate(http_requests_total{status=~\"4xx|5xx\"}[5m])) / sum(rate(http_requests_total[5m]))) * 100 or vector(0)"}],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {"mode": "absolute", "steps": [{"value": 0, "color": "green"}, {"value": 1, "color": "yellow"}, {"value": 5, "color": "red"}]}
        }
      },
      "options": {"graphMode": "area", "colorMode": "value"}
    },
    {
      "id": 10,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 4},
      "type": "row",
      "title": "Routes API (Backend)",
      "collapsed": false
    },
    {
      "id": 11,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 5},
      "type": "bargauge",
      "title": "Top 10 Routes (1h)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "topk(10, sum by (path) (increase(http_requests_total[1h])))", "legendFormat": "{{path}}"}],
      "options": {"orientation": "horizontal", "displayMode": "gradient"},
      "fieldConfig": {"defaults": {"unit": "short"}}
    },
    {
      "id": 12,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 5},
      "type": "timeseries",
      "title": "Requetes/sec par Route",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "sum by (path) (rate(http_requests_total[1m]))", "legendFormat": "{{path}}"}],
      "fieldConfig": {"defaults": {"unit": "reqps", "custom": {"lineWidth": 2, "fillOpacity": 10}}}
    },
    {
      "id": 13,
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 13},
      "type": "timeseries",
      "title": "Latence (P50, P95, P99)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P50"},
        {"expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P95"},
        {"expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "P99"}
      ],
      "fieldConfig": {"defaults": {"unit": "s", "custom": {"lineWidth": 2, "fillOpacity": 10}}}
    },
    {
      "id": 14,
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 13},
      "type": "piechart",
      "title": "Repartition Status HTTP",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "sum by (status) (increase(http_requests_total[1h]))", "legendFormat": "{{status}}"}],
      "options": {"legend": {"displayMode": "table", "placement": "right"}}
    },
    {
      "id": 20,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 19},
      "type": "row",
      "title": "Traefik (Reverse Proxy)",
      "collapsed": false
    },
    {
      "id": 21,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "type": "bargauge",
      "title": "Top Routes Traefik (1h)",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "topk(10, sum by (router) (increase(traefik_router_requests_total[1h])))", "legendFormat": "{{router}}"}],
      "options": {"orientation": "horizontal", "displayMode": "gradient"},
      "fieldConfig": {"defaults": {"unit": "short"}}
    },
    {
      "id": 22,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "type": "timeseries",
      "title": "Traefik Requetes/sec",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "sum by (service) (rate(traefik_service_requests_total[1m]))", "legendFormat": "{{service}}"}],
      "fieldConfig": {"defaults": {"unit": "reqps", "custom": {"lineWidth": 2, "fillOpacity": 10}}}
    },
    {
      "id": 30,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 28},
      "type": "row",
      "title": "Infrastructure (Containers)",
      "collapsed": false
    },
    {
      "id": 31,
      "gridPos": {"h": 7, "w": 12, "x": 0, "y": 29},
      "type": "timeseries",
      "title": "CPU %",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "rate(container_cpu_usage_seconds_total{name=~\"timemanager-.*\"}[1m]) * 100", "legendFormat": "{{name}}"}],
      "fieldConfig": {"defaults": {"unit": "percent", "custom": {"lineWidth": 2, "fillOpacity": 10}}}
    },
    {
      "id": 32,
      "gridPos": {"h": 7, "w": 12, "x": 12, "y": 29},
      "type": "timeseries",
      "title": "Memoire",
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{"expr": "container_memory_usage_bytes{name=~\"timemanager-.*\"}", "legendFormat": "{{name}}"}],
      "fieldConfig": {"defaults": {"unit": "bytes", "custom": {"lineWidth": 2, "fillOpacity": 10}}}
    },
    {
      "id": 40,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 36},
      "type": "row",
      "title": "Logs",
      "collapsed": false
    },
    {
      "id": 41,
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 37},
      "type": "logs",
      "title": "Backend Logs",
      "datasource": {"type": "loki", "uid": "loki"},
      "targets": [{"expr": "{container=\"timemanager-backend\"} | json", "refId": "A"}],
      "options": {"showTime": true, "showLabels": false, "wrapLogMessage": true, "prettifyLogMessage": true, "enableLogDetails": true, "sortOrder": "Descending"}
    }
  ]
}
